{
  "name": "@wppconnect/server-cli",
  "version": "1.3.7",
  "description": "Servidor de WPPConnect com frontend para ser executado a partir de uma linha de comando",
  "bin": {
    "wppserver": "./bin/wppserver.js"
  },
  "main": "dist/index.js",
  "author": "edgardmessias",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rimraf dist && babel src -s -D -d dist",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "license:add": "license-check-and-add add",
    "license:check": "license-check-and-add check",
    "lint": "eslint --ext .js src",
    "prepare": "husky install",
    "release": "release-it",
    "start": "cross-env RUN_SERVER=1 babel-node src",
    "watch": "nodemon -w src --exec \"babel-node src\""
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.22.11",
    "@wppconnect/server": "^2.2.4",
    "commander": "^11.0.0",
    "merge-deep": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/eslint-plugin": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@wppconnect/frontend": "^1.0.3",
    "aws-sdk": "^2.1440.0",
    "crypto-js": "^4.1.1",
    "mongoose": "^7.4.5",
    "redis": "^4.6.8",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "license-check-and-add": "^4.0.5",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "release-it": "^15.11.0",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1440.0",
    "crypto-js": "^4.1.1",
    "mongoose": "^7.4.5",
    "redis": "^4.6.8"
  }
}
